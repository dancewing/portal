<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.mail.model.Cyrus" />
	<import class="com.liferay.mail.model.CyrusUser" />
	<import class="com.liferay.mail.model.CyrusVirtual" />
	<import class="com.liferay.mail.model.Mail" />
	<class name="com.liferay.mail.model.impl.CyrusUserImpl" table="Mail_CyrusUser">
		<cache usage="read-write" />
		<id name="userId" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property name="password" type="com.liferay.portal.dao.orm.hibernate.StringType" column="password_" />
	</class>
	<class name="com.liferay.mail.model.impl.CyrusVirtualImpl" table="Mail_CyrusVirtual">
		<cache usage="read-write" />
		<id name="emailAddress" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>