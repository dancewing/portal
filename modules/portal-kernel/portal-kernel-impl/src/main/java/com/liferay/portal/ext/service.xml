<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.liferay.portal.ext">
	<author>Jeff Qian</author>	
	<namespace>PortalExt</namespace>
	<entity name="FilterScope" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="filterScopeId" type="long" primary="true" />

		<!-- Group instance -->

		<!-- Audit fields -->

		<!-- Other fields -->
		<column name="filterName" type="String" />
		<column name="className" type="String" />
		<column name="contextPath" type="String" />
		<!-- Order -->

		<order by="desc">
			<order-column name="className" />
		</order>

		<!-- Finder methods -->	
		
		<finder name="ContextPath" return-type="Collection">
			<finder-column name="contextPath" />
		</finder>	
			
		<finder name="FCC" return-type="FilterScope" unique="true">
			<finder-column name="filterName" />
			<finder-column name="className" />
			<finder-column name="contextPath" />
		</finder>		
		
		<finder name="ClassName" return-type="Collection">
			<finder-column name="className" />
		</finder>
		

	</entity>	
	<entity name="FilterGroup" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="filterGroupId" type="long" primary="true" />

		<!-- Audit fields -->

		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="groupId" type="long" />
		<column name="filterScopeId" type="long" />

		<!-- Relationships -->
		<!-- Order -->

		<order by="asc">
			<order-column name="createDate" />
		</order>

		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId" />
		</finder>
		<finder name="FilterScopeId" return-type="Collection">
			<finder-column name="filterScopeId" />
		</finder>
		<finder name="F_G" return-type="FilterGroup" unique="true">
			<finder-column name="groupId" />
			<finder-column name="filterScopeId" />
		</finder>
	</entity>
	
	<!-- entities for build connections for models -->
	<entity name="ModelConnectionEntry" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="entryId" type="long" primary="true" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="entryGroupId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="classNameId" type="long" />
		<column name="classPK" type="long" />

		<!-- Finder methods -->

		<finder name="C_C" return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>
		<finder name="C_C_EG" return-type="ModelConnectionEntry" unique="true">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
			<finder-column name="entryGroupId" />
		</finder>		

		<!-- References -->

		<reference package-path="com.liferay.portal" entity="ClassName" />
	</entity>	

	<entity name="ModelConnectionLine" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="lineId" type="long" primary="true" />

		<!-- Audit fields -->
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->
		<column name="lineGroupId" type="long" />
		<column name="classNameId" type="long" />
		<column name="classPK" type="long" />

		<!-- Finder methods -->

		<finder name="C_C" return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>
		<finder name="C_C_LG" return-type="ModelConnectionLine" unique="true">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
			<finder-column name="lineGroupId" />
		</finder>		

		<!-- References -->

		<reference package-path="com.liferay.portal" entity="ClassName" />
	</entity>
	

	<!-- Service Integration start -->
	<entity name="ServiceHost" local-service="true" remote-service="false">
		<column name="hostId" type="long" primary="true" />
		<!-- PK fields -->
		<column name="code" type="String"/>
		<!-- Audit fields -->
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<!-- Other fields -->
		<column name="address" type="String" />
		<column name="protocol" type="String" db-name="protocol_"/>
		<column name="port" type="int" db-name="port_" />
		
		<column name="proxyHost" type="String"/>
		<column name="proxyPort" type="int"/>
		<column name="proxyUserName" type="String" />
		<column name="proxyPassword" type="String" />
		<column name="proxyAuthType" type="String" />
		
		<column name="cacheResponse" type="boolean" />
		
		<column name="maxConnectionsPerHost" type="int" />
		<column name="timeout" type="int" />
		
		<column name="sessionCookieUseFullHostName" db-name="scufh" type="boolean"/>

		
		<finder name="Code" return-type="ServiceHost" unique="true">
			<finder-column name="code" />
		</finder>

	</entity>

	<entity name="ServiceTag" local-service="true" remote-service="false">
		
		<!-- PK fields -->
		<column name="tagId" type="long" primary="true" />
		<!-- Audit fields -->
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<!-- Other fields -->
		<column name="tagName" type="String" />
		
		<finder name="TagName" return-type="ServiceTag" unique="true">
			<finder-column name="tagName" />
		</finder>

	</entity>
			
	<entity name="ServiceAuthorization" local-service="true" remote-service="false">
		<column name="authId" type="long" primary="true" />
		<!-- PK fields -->
		<column name="code" type="String"/>

		<!-- Audit fields -->
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- serviceType for remote / local -->
		<column name="classNameId" type="long"/>
		<column name="classPK" type="long" />

		<!-- Other fields -->
		<column name="type" type="String" db-name="type_"/>
		<column name="template" type="String" />

		<!-- Finder methods -->
		<finder name="Code" return-type="ServiceAuthorization" unique="true">
			<finder-column name="code" />
		</finder>		
	</entity>
		
	<entity name="ServiceRemoteDef" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="remoteId" type="long" primary="true" />	
		<column name="code" type="String"/>

		<!-- Audit fields -->
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<column name="tagId" type="long"/>
		
		<!-- Other fields for remote service -->
		<column name="host" type="long" db-name="host_" />
		<column name="path" type="String" db-name="path_" />
		<column name="httpMethod" type="String"/>
		<column name="rawString" type="String"/>
		
		<column name="description" type="String"/>
		<column name="deprecated" type="boolean"/>
		
		<column name="sendWithCookies" type="boolean"/>
		<column name="cookieDomain" type="String"/>
		<column name="cookiePath" type="String"/>
				
		<column name="scriptParameter" type="String"/>
		<column name="scriptHeader" type="String"/>
		<column name="scriptResponse" type="String"/>
		
		<!-- cache setting -->
		<column name="cacheLiveTime" type="int"/>
		
		<column name="authId" type="long"/>
		
		<!-- Finder methods -->
		<finder name="Code" return-type="ServiceRemoteDef" unique="true">
			<finder-column name="code" />
		</finder>
					
		<finder name="AuthId" return-type="ServiceAuthorization">
			<finder-column name="authId" />
		</finder>
		
		<finder name="TagId" return-type="Collection">
			<finder-column name="tagId" />
		</finder>
		
	</entity>
		
	<entity name="ServicePublishDef" local-service="true" remote-service="false">

		<!-- PK fields -->
		<column name="publishId" type="long" primary="true" />	
		<column name="path" type="String" db-name="path_" />

		<!-- Audit fields -->
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<column name="tagId" type="long"/>

		<!-- Other fields for remote service -->
		
		<column name="httpMethod" type="String"/>
		<column name="scriptResponse" type="String"/>
		<column name="siteAwareness" type="boolean" />
		<column name="supportAnonymous" type="boolean" />

		<column name="description" type="String"/>
		<column name="deprecated" type="boolean"/>

		<!-- cache setting -->
		<column name="cacheLiveTime" type="int"/>
				
		<finder name="Path" return-type="ServicePublishDef" unique="true">
			<finder-column name="path" />
		</finder>

		<finder name="TagId" return-type="Collection">
			<finder-column name="tagId" />
		</finder>
		
		<reference package-path="com.liferay.portal" entity="Group"/>
					
	</entity>

	<entity name="ServiceParam" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="paramId" type="long" primary="true" />

		<!-- Audit fields -->
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />


		<!-- serviceType for remote / local -->
		<column name="classNameId" type="long"/>
		<column name="classPK" type="long" />
				
		<!-- Other fields for remote service -->
		<column name="paramType" type="String"/>
		<column name="paramName" type="String"/>
		<column name="paramValue" type="String"/>
		<column name="paramObject" type="String"/>
		<column name="paramDesc" type="String" />
		<column name="defaultValue" type="String" />
		
		<!-- Finder methods -->
		<finder name="C_C_P" return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
			<finder-column name="paramType" />
		</finder>
		
		<finder name="C_C" return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>		
	</entity>
	
	<entity name="ServiceHistory" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="hisId" type="long" primary="true" />

		<!-- Audit fields -->
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />


		<!-- serviceType for remote / local -->
		<column name="classNameId" type="long"/>
		<column name="classPK" type="long" />
				
		<!-- Other fields for remote service -->
		<column name="page" type="String"/>
		<column name="friendlyURL" type="String"/>
		<column name="portletName" type="String"/>
		<column name="statusCode" type="int"/> <!-- response status code -->
		<column name="timeCost" type="int"/> <!-- total cost for service invocation -->
		<column name="responseText" type="String" /> <!-- original response from Service  -->
		<column name="wrappedText" type="String" /> <!-- response error code -->
		<column name="debugInfo" type="String" /> <!-- just only for service debug is checked response from Service  -->
		
		<!-- Finder methods -->

		<finder name="C_C" return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>		
	</entity>	
	<!-- Service Integration End -->					
</service-builder>